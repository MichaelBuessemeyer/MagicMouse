    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "eslint": "^8.5.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^8.3.0",
    "pkg": "^5.5.1",
    "prettier": "^2.5.1"
  }
